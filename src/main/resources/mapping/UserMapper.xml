<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.maxiao.consmetic.mapper.UserMapper">
    <resultMap id="User" type="com.maxiao.consmetic.domain.User">
        <result column="user_id" property="userId" jdbcType="VARCHAR"/>
        <result column="user_name" property="userName" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="queryUser" parameterType="String" resultMap="User">
      SELECT * FROM tb_cosmetic_user t WHERE t.user_id =#{userId,jdbcType=VARCHAR}
    </select>
</mapper>